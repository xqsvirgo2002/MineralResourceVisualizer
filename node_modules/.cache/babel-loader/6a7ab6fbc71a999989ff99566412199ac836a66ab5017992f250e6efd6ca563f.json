{"ast":null,"code":"import * as echarts from 'echarts';\nimport mineralsData from '@/path/to/minerals_production.json'; // 根据实际路径更新\nimport { Dialog } from 'element-ui';\nexport default {\n  name: 'WorldMap',\n  components: {\n    [Dialog.name]: Dialog\n  },\n  data() {\n    return {\n      selectedCountry: '',\n      selectedCountryMinerals: [],\n      dialogVisible: false\n    };\n  },\n  mounted() {\n    // 初始化 ECharts 地图\n    var chart = echarts.init(document.getElementById('map'));\n    var option = {\n      series: [{\n        type: 'map',\n        map: 'world'\n        // 其他 ECharts 配置...\n      }]\n    };\n    chart.setOption(option);\n    chart.on('click', this.handleCountryClick);\n  },\n  methods: {\n    handleCountryClick(params) {\n      this.selectedCountry = params.name;\n      this.selectedCountryMinerals = mineralsData[this.selectedCountry] || [];\n      this.dialogVisible = true;\n    },\n    handleDialogClose() {\n      this.dialogVisible = false;\n    }\n  }\n};","map":{"version":3,"names":["echarts","mineralsData","Dialog","name","components","data","selectedCountry","selectedCountryMinerals","dialogVisible","mounted","chart","init","document","getElementById","option","series","type","map","setOption","on","handleCountryClick","methods","params","handleDialogClose"],"sources":["src/components/WorldMap.vue"],"sourcesContent":["<template>\n  <div class=\"map-container\">\n    <div id=\"map\"></div>\n    <el-dialog\n      :visible.sync=\"dialogVisible\"\n      :title=\"selectedCountry + ' 的矿产资源'\"\n      width=\"60%\"\n      @close=\"handleDialogClose\">\n      <ul>\n        <li v-for=\"mineral in selectedCountryMinerals\" :key=\"mineral.Minerals\">\n          {{ mineral.Minerals }}: {{ mineral.Production }}\n        </li>\n      </ul>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport * as echarts from 'echarts';\nimport mineralsData from '@/path/to/minerals_production.json'; // 根据实际路径更新\nimport { Dialog } from 'element-ui';\n\nexport default {\n  name: 'WorldMap',\n  components: {\n    [Dialog.name]: Dialog\n  },\n  data() {\n    return {\n      selectedCountry: '',\n      selectedCountryMinerals: [],\n      dialogVisible: false\n    };\n  },\n  mounted() {\n    // 初始化 ECharts 地图\n    var chart = echarts.init(document.getElementById('map'));\n    var option = {\n      series: [\n        {\n          type: 'map',\n          map: 'world',\n          // 其他 ECharts 配置...\n        }\n      ]\n    };\n\n    chart.setOption(option);\n    chart.on('click', this.handleCountryClick);\n  },\n  methods: {\n    handleCountryClick(params) {\n      this.selectedCountry = params.name;\n      this.selectedCountryMinerals = mineralsData[this.selectedCountry] || [];\n      this.dialogVisible = true;\n    },\n    handleDialogClose() {\n      this.dialogVisible = false;\n    }\n  }\n}\n</script>\n\n<style>\n.map-container {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  height: 70vh;\n}\n\n#map {\n  width: 1000px;\n  height: 800px;\n}\n</style>\n"],"mappings":"AAkBA,YAAAA,OAAA;AACA,OAAAC,YAAA;AACA,SAAAC,MAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACA,CAAAF,MAAA,CAAAC,IAAA,GAAAD;EACA;EACAG,KAAA;IACA;MACAC,eAAA;MACAC,uBAAA;MACAC,aAAA;IACA;EACA;EACAC,QAAA;IACA;IACA,IAAAC,KAAA,GAAAV,OAAA,CAAAW,IAAA,CAAAC,QAAA,CAAAC,cAAA;IACA,IAAAC,MAAA;MACAC,MAAA,GACA;QACAC,IAAA;QACAC,GAAA;QACA;MACA;IAEA;IAEAP,KAAA,CAAAQ,SAAA,CAAAJ,MAAA;IACAJ,KAAA,CAAAS,EAAA,eAAAC,kBAAA;EACA;EACAC,OAAA;IACAD,mBAAAE,MAAA;MACA,KAAAhB,eAAA,GAAAgB,MAAA,CAAAnB,IAAA;MACA,KAAAI,uBAAA,GAAAN,YAAA,MAAAK,eAAA;MACA,KAAAE,aAAA;IACA;IACAe,kBAAA;MACA,KAAAf,aAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}
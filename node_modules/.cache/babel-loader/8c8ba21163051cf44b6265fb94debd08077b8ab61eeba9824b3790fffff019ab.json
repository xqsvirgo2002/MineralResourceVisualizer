 },\n    methods: {\n        initBarChart() {\n            const chartDom = this.$refs.GlobalMineralChart;\n            const myChart = echarts.init(chartDom);\n            const options = {\n                // ECharts 配置项\n                xAxis: {\n                    type: 'category',\n                    data: Object.keys(this.productionData)\n                },\n                yAxis: {\n                    type: 'value'\n                },\n                series: [{\n                    data: Object.values(this.productionData).map(item => item['2021']), // 这假设我们只显示 2021 年的数据\n                    type: 'bar'\n                }]\n            };\n            myChart.setOption(options);\n        }\n    }\n};\n</script>\n\n<style scoped>\n.chart-card {\n    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\n    margin-bottom: 20px;\n}\n\n.chart {\n    width: 100%;\n    height: 400px;\n}\n</style>\n"],"mappings":"AAgBA,YAAAA,OAAA;AACA,OAAAC,cAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAF;IACA;EACA;EACAG,QAAA;IACA,KAAAC,YAAA;EACA;EACAC,OAAA;IACAD,aAAA;MACA,MAAAE,QAAA,QAAAC,KAAA,CAAAC,kBAAA;MACA,MAAAC,OAAA,GAAAV,OAAA,CAAAW,IAAA,CAAAJ,QAAA;MACA,MAAAK,OAAA;QACA;QACAC,KAAA;UACAC,IAAA;UACAX,IAAA,EAAAY,MAAA,CAAAC,IAAA,MAAAf,cAAA;QACA;QACAgB,KAAA;UACAH,IAAA;QACA;QACAI,MAAA;UACAf,IAAA,EAAAY,MAAA,CAAAI,MAAA,MAAAlB,cAAA,EAAAmB,GAAA,CAAAC,IAAA,IAAAA,IAAA;UAAA;UACAP,IAAA;QACA;MACA;MACAJ,OAAA,CAAAY,SAAA,CAAAV,OAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}
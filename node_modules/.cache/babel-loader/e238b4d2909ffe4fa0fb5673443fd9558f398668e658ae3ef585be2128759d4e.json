unction (params) {\n            // 在tooltip中显示详细内容\n            let content = params[0].name + '<br/>';\n            params.forEach(function (item) {\n              content += item.seriesName + ': ' + item.value + '<br/>';\n            });\n            return content;\n          }\n        },\n        title: {\n          text: '有色金属的全球生产'\n        },\n        xAxis: {\n          type: 'category',\n          data: this.chart2Data.Commodity\n        },\n        yAxis: {\n          type: 'value'\n        },\n        series: [{\n          data: this.chart2Data['2017'],\n          type: 'bar',\n          name: '2017'\n        }, {\n          data: this.chart2Data['2018'],\n          type: 'bar',\n          name: '2018'\n        }, {\n          data: this.chart2Data['2019'],\n          type: 'bar',\n          name: '2019'\n        }, {\n          data: this.chart2Data['2020'],\n          type: 'bar',\n          name: '2020'\n        }, {\n          data: this.chart2Data['2021'],\n          type: 'bar',\n          name: '2021'\n        }]\n      };\n      this.chart2Instance.setOption(option2);\n    },\n    // 初始化第三个图表\n    initChart3() {\n      const option3 = {\n        tooltip: {\n          trigger: 'axis',\n          // 设置tooltip的触发方式，'axis'表示鼠标悬停在坐标轴上时显示tooltip\n          formatter: function (params) {\n            // 在tooltip中显示详细内容\n            let content = params[0].name + '<br/>';\n            params.forEach(function (item) {\n              content += item.seriesName + ': ' + item.value + '<br/>';\n            });\n            return content;\n          }\n        },\n        title: {\n          text: '贵金属的全球生产'\n        },\n        xAxis: {\n          type: 'category',\n          data: this.chart3Data.Commodity\n        },\n        yAxis: {\n          type: 'value'\n        },\n        series: [{\n          data: this.chart3Data['2017'],\n          type: 'bar',\n          name: '2017'\n        }, {\n          data: this.chart3Data['2018'],\n          type: 'bar',\n          name: '2018'\n        }, {\n          data: this.chart3Data['2019'],\n          type: 'bar',\n          name: '2019'\n        }, {\n          data: this.chart3Data['2020'],\n          type: 'bar',\n          name: '2020'\n        }, {\n          data: this.chart3Data['2021'],\n          type: 'bar',\n          name: '2021'\n        }]\n      };\n      this.chart3Instance.setOption(option3);\n    },\n    // 初始化第四个图表\n    initChart4() {\n      const option4 = {\n        tooltip: {\n          trigger: 'axis',\n          // 设置tooltip的触发方式，'axis'表示鼠标悬停在坐标轴上时显示tooltip\n          formatter: function (params) {\n            // 在tooltip中显示详细内容\n            let content = params[0].name + '<br/>';\n            params.forEach(function (item) {\n              content += item.seriesName + ': ' + item.value + '<br/>';\n            });\n            return content;\n          }\n        },\n        title: {\n          text: '矿物燃料的全球生产'\n        },\n        xAxis: {\n          type: 'category',\n          data: this.chart4Data.Commodity\n        },\n        yAxis: {\n          type: 'value'\n        },\n        series: [{\n          data: this.chart4Data['2017'],\n          type: 'bar',\n          name: '2017'\n        }, {\n          data: this.chart4Data['2018'],\n          type: 'bar',\n          name: '2018'\n        }, {\n          data: this.chart4Data['2019'],\n          type: 'bar',\n          name: '2019'\n        }, {\n          data: this.chart4Data['2020'],\n          type: 'bar',\n          name: '2020'\n        }, {\n          data: this.chart4Data['2021'],\n          type: 'bar',\n          name: '2021'\n        }]\n      };\n      this.chart4Instance.setOption(option4);\n    }\n  }\n};","map":{"version":3,"names":["echarts","chart1Data","chart2Data","chart3Data","chart4Data","name","data","mounted","chart1Instance","init","$refs","chart1","initChart1","chart2Instance","chart2","initChart2","chart3Instance","chart3","initChart3","chart4Instance","chart4","
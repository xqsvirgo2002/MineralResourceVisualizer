{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport * as echarts from 'echarts';\nexport default {\n  name: 'MiningAreaChart',\n  data() {\n    return {\n      chartData: {\n        countries: [],\n        areas: []\n      }\n    };\n  },\n  mounted() {\n    console.log(\"Component mounted, loading chart data...\");\n    this.loadChartData();\n  },\n  methods: {\n    loadChartData() {\n      axios.get('public/mining_area_data.json') // 更改为JSON文件的实际路径\n      .then(response => {\n        response.data.forEach(row => {\n          this.chartData.countries.push(row.COUNTRY_NAME);\n          this.chartData.areas.push(row.AREA);\n        });\n        this.createChart();\n      }).catch(error => console.error('Error loading the JSON file:', error));\n    },\n    createChart() {\n      const chartDom = document.getElementById('chart');\n      const myChart = echarts.init(chartDom);\n      const option = {\n        xAxis: {\n          type: 'category',\n          data: this.chartData.countries\n        },\n        yAxis: {\n          type: 'value'\n        },\n        series: [{\n          data: this.chartData.areas,\n          type: 'bar'\n        }]\n      };\n      myChart.setOption(option);\n    }\n  }\n};","map":{"version":3,"names":["axios","echarts","name","data","chartData","countries","areas","mounted","console","log","loadChartData","methods","get","then","response","forEach","row","push","COUNTRY_NAME","AREA","createChart","catch","error","chartDom","document","getElementById","myChart","init","option","xAxis","type","yAxis","series","setOption"],"sources":["src/components/MiningAreaChart.vue"],"sourcesContent":["<template>\n  <div>\n    <h2>矿区面积分布</h2>\n    <div id=\"chart\" style=\"height: 400px; width: 100%;\"></div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport * as echarts from 'echarts';\n\nexport default {\n  name: 'MiningAreaChart',\n  data() {\n    return {\n      chartData: {\n        countries: [],\n        areas: []\n      }\n    }\n  },\n  mounted() {\n    console.log(\"Component mounted, loading chart data...\");\n    this.loadChartData();\n  },\n  methods: {\n    loadChartData() {\n      axios.get('public/mining_area_data.json') // 更改为JSON文件的实际路径\n        .then(response => {\n          response.data.forEach(row => {\n            this.chartData.countries.push(row.COUNTRY_NAME);\n            this.chartData.areas.push(row.AREA);\n          });\n          this.createChart();\n        })\n        .catch(error => console.error('Error loading the JSON file:', error));\n    },\n    createChart() {\n      \n      const chartDom = document.getElementById('chart');\n      const myChart = echarts.init(chartDom);\n      const option = {\n        xAxis: {\n          type: 'category',\n          data: this.chartData.countries\n        },\n        yAxis: {\n          type: 'value'\n        },\n        series: [\n          {\n            data: this.chartData.areas,\n            type: 'bar'\n          }\n        ]\n      };\n      myChart.setOption(option);\n    }\n  }\n}\n</script>\n\n<style>\n/* 样式 */\n</style>\n"],"mappings":";AAQA,OAAAA,KAAA;AACA,YAAAC,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;QACAC,SAAA;QACAC,KAAA;MACA;IACA;EACA;EACAC,QAAA;IACAC,OAAA,CAAAC,GAAA;IACA,KAAAC,aAAA;EACA;EACAC,OAAA;IACAD,cAAA;MACAV,KAAA,CAAAY,GAAA;MAAA,CACAC,IAAA,CAAAC,QAAA;QACAA,QAAA,CAAAX,IAAA,CAAAY,OAAA,CAAAC,GAAA;UACA,KAAAZ,SAAA,CAAAC,SAAA,CAAAY,IAAA,CAAAD,GAAA,CAAAE,YAAA;UACA,KAAAd,SAAA,CAAAE,KAAA,CAAAW,IAAA,CAAAD,GAAA,CAAAG,IAAA;QACA;QACA,KAAAC,WAAA;MACA,GACAC,KAAA,CAAAC,KAAA,IAAAd,OAAA,CAAAc,KAAA,iCAAAA,KAAA;IACA;IACAF,YAAA;MAEA,MAAAG,QAAA,GAAAC,QAAA,CAAAC,cAAA;MACA,MAAAC,OAAA,GAAAzB,OAAA,CAAA0B,IAAA,CAAAJ,QAAA;MACA,MAAAK,MAAA;QACAC,KAAA;UACAC,IAAA;UACA3B,IAAA,OAAAC,SAAA,CAAAC;QACA;QACA0B,KAAA;UACAD,IAAA;QACA;QACAE,MAAA,GACA;UACA7B,IAAA,OAAAC,SAAA,CAAAE,KAAA;UACAwB,IAAA;QACA;MAEA;MACAJ,OAAA,CAAAO,SAAA,CAAAL,MAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"modu